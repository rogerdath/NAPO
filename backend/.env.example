# File System
OUTPUT_DIR=/output
LOGS_DIR=/logs
RAW_FILES_DIR=/raw
UPLOAD_DIR=/uploads
TEMP_DIR=/tmp

# Logging
LOG_LEVEL=info
MAX_LOG_SIZE=5242880  # 5MB in bytes
MAX_LOG_FILES=5
LOG_FORMAT=json
LOG_RETENTION_DAYS=30

# CSV Processing
DEFAULT_ENCODING=utf8
FIELD_SEPARATOR=,
QUOTE_CHAR="
ESCAPE_CHAR=\
MAX_FILE_SIZE=100000000
ALLOWED_FILE_TYPES=.csv
REQUIRED_FIELDS=OMRADEKODE,AVTALEOMRADEKODE
MAX_ROWS_PER_BATCH=1000
PRESERVE_NULL_VALUES=true
NORMALIZE_HEADERS=true

# Security
API_KEY_HEADER=X-API-Key
JWT_SECRET=your-jwt-secret
ENCRYPTION_KEY=your-secret-key
CORS_ORIGINS=http://localhost:3200
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60
MAX_FILE_UPLOADS=10

# Database
DB_TYPE=postgresql
DB_HOST=localhost
DB_PORT=5432
DB_NAME=avtaleportal
DB_USER=dbuser
DB_PASSWORD=dbpassword
DB_SSL=true
DB_MAX_CONNECTIONS=10
DB_IDLE_TIMEOUT=30000

# Cache
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=redispass
CACHE_TTL=3600 